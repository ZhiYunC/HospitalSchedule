@using Demo.Models;
@{
    List<Schedule> schedules = ViewBag.schedules;
}

@if (schedules != null)
{
    foreach (Schedule schedule in schedules)
    {
        <div>
            <span>@schedule.Schedule_id</span>
            <span>@schedule.Schedule_date</span>
            <span>@schedule.Schedule_doctor_name</span>
        </div>
    }
}
else
{
    <p>沒有資料！</p>
}

